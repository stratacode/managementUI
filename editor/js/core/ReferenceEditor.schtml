<div id="ReferenceEditor" class="elementEditor">
   <span class="formFieldLabel" style="position: relative; left: -8px">
      <img visible=":= icon != null" src=':= iconPath' alt=':= iconAlt'/>
      <%= displayName %>
   </span>
   <a class="textFieldValue" clickEvent="=: gotoReference()"><%= referenceId %></a>
</div>
